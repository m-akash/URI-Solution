#include <stdio.h>
int main()
{
    int i, count = 0;
    float sum=0,a;

    for (i = 0; i < 6; i++)
    {
        scanf("%f", &a);

        if (a >= 0)
            count++;

            if(a>=0){
                sum = sum + a;
            }
    }
    printf("%d valores positivos\n", count);
    printf("%.1f\n",sum/count);


    return 0;
}
